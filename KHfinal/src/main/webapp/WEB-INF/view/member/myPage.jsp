<script type="text/javascript">
    // 만들 팝업창 width 크기의 1/2 만큼 보정값으로 빼주었음
    var popupWidth = 470;
    var popupHeight = 720;
    var popupX = Math.ceil((window.screen.width - popupWidth) / 2);
    var popupY = Math.ceil((window.screen.height - popupHeight) / 2);

    $("#MICT_Start").click(function (e) {
        var agent = navigator.userAgent.toLowerCase();

        if ((navigator.appName == 'Netscape' && agent.indexOf('trident') != -1) || (agent.indexOf("msie") != -1)) {
            alert('Internet Explorer는 지원하지 않는 환경입니다. Internet Explorer 가 아닌 다른 브라우저를 사용해 주세요');
            return;
        } else {
            MICT_Insert(1, 'Start');
        }
    });

    $("#MICT_Result").click(function (e) {
        var agent = navigator.userAgent.toLowerCase();

        if ((navigator.appName == 'Netscape' && agent.indexOf('trident') != -1) || (agent.indexOf("msie") != -1)) {
            alert('Internet Explorer는 지원하지 않는 환경입니다. Internet Explorer 가 아닌 다른 브라우저를 사용해 주세요');
            return;
        }

        MICT_Insert(1, 'Result');
        //var MictURL = 'https://mict.jobkorea.co.kr' + "/result/?token=" + '';
        //window.open(MictURL, 'new', 'width =' + popupWidth + ', height =' + popupHeight  + ', left=' + popupX + ', top=' + popupY);
    });

    $("#MICT_Restart").click(function (e) {
        var agent = navigator.userAgent.toLowerCase();

        if ((navigator.appName == 'Netscape' && agent.indexOf('trident') != -1) || (agent.indexOf("msie") != -1)) {
            alert('Internet Explorer는 지원하지 않는 환경입니다. Internet Explorer 가 아닌 다른 브라우저를 사용해 주세요');
            return;
        } else {
            MICT_Insert(1, 'Start');
        }
    });

    function onMictPopupClose() {
        location.href = "https://" + location.host + '/user/mypage';
    }

    var mypage = null;
    var timer = null;

    function MICT_Insert(tokenType, type) {
        $.ajax({
            type: "GET",
            cache: false,
            async: false,
            data: {
                tokenType: tokenType
            },
            url: "/user/mypage/MICTInsert",
        }).done(function (data) {
            let MictURL = '';
            var token = data.token;

            if (tokenType == 1 && type == 'Start') {
                MictURL = 'https://mict.jobkorea.co.kr' + "/?token=" + token;
            } else {
                MictURL = 'https://mict.jobkorea.co.kr' + "/result/?token=" + token;
            }

            mypage = window.open(MictURL, 'new', 'width =' + popupWidth + ', height =' + popupHeight + ', left=' + popupX + ', top=' + popupY);

            if (tokenType == 1 && type == 'Start') {
                timer = setInterval(mypage_checkChild, 1000);
            }
        });
    }

    function mypage_checkChild() {
        if (mypage.closed) {
            clearInterval(timer);
            location.href = "https://" + location.host + '/user/mypage';
        }
    }
</script>
